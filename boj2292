#include <iostream>
using namespace std;

int pow(int n)
{
  
  return 3*n*n-3*n+1;
  
}



int main() 
{

  int n;
  cin >>n;
  
  int idx=1;
  while(1)
  {
    
    if( n<=pow(idx))
        {
          cout<<idx;
          return 0;
          
          
        }
    
    
    idx++;
    
  }
  
  
  
  
	return 0;
}
